{
    "name": "dashboard-test-ionc",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test.e2e": "cypress run",
        "test.unit": "vitest",
        "lint": "eslint",
        "electron": "electron electron/main.js",
        "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development electron electron/main.js\"",
        "electron:pack": "npm run build && node scripts/fix-electron-build.js && electron-builder",
        "electron:dist": "npm run build && node scripts/fix-electron-build.js && electron-builder --publish=never",
        "electron:build": "npm run build && node scripts/fix-electron-build.js",
        "android:sync": "npx cap sync android",
        "android:run": "npx cap run android",
        "android:build": "npm run build && npx cap sync android && npx cap run android"
    },
    "dependencies": {
        "@capacitor/android": "^7.4.3",
        "@capacitor/app": "7.1.0",
        "@capacitor/camera": "^7.0.2",
        "@capacitor/core": "7.4.3",
        "@capacitor/filesystem": "^7.1.4",
        "@capacitor/haptics": "7.0.2",
        "@capacitor/keyboard": "7.0.3",
        "@capacitor/network": "^7.0.2",
        "@capacitor/preferences": "^7.0.2",
        "@capacitor/status-bar": "7.0.3",
        "@ionic/pwa-elements": "^3.3.0",
        "@ionic/react": "^8.5.0",
        "@ionic/react-router": "^8.5.0",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "ionicons": "^7.4.0",
        "jspdf": "^3.0.2",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4"
    },
    "devDependencies": {
        "@capacitor/cli": "7.4.3",
        "@testing-library/dom": ">=7.21.4",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jspdf": "^2.0.0",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@vitejs/plugin-legacy": "^5.0.0",
        "@vitejs/plugin-react": "^4.0.1",
        "concurrently": "^9.2.1",
        "cypress": "^13.5.0",
        "electron": "^38.1.0",
        "electron-builder": "^26.0.12",
        "eslint": "^9.20.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "jsdom": "^22.1.0",
        "terser": "^5.4.0",
        "typescript": "^5.1.6",
        "typescript-eslint": "^8.24.0",
        "vite": "~5.2.0",
        "vitest": "^0.34.6",
        "wait-on": "^8.0.5"
    },
    "overrides": {
        "rollup": "4.44.0"
    },
    "description": "An Ionic project",
    "main": "electron/main.js",
    "homepage": "./",
    "build": {
        "appId": "com.dashboard.test.ionic",
        "productName": "Dashboard Test Ionic",
        "directories": {
            "output": "dist-electron"
        },
        "files": [
            "dist/**/*",
            "electron/**/*",
            "node_modules/**/*"
        ],
        "win": {
            "icon": "public/favicon.png",
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "mac": {
            "icon": "public/favicon.png",
            "category": "public.app-category.productivity"
        },
        "linux": {
            "icon": "public/favicon.png",
            "target": [
                {
                    "target": "AppImage",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}
